{
    "name": "scottpledger/phpdoc-md",
    "description" : "PHP documentation generator, with markdown output",
    "keywords" : [ "PHP", "Markdown", "PHPDoc" ],
    "homepage" : "https://github.com/scottpledger/phpdoc-md",
    "license" : "MIT",
    "require" : {
        "php" : ">=5.3.1",
        "twig/twig" : "~1.16.0"
    },
    "require-dev" : {
        "phpdocumentor/phpdocumentor" : "~2.7.0"
    },
    "authors" : [
		{
			"name" : "Scott Pledger",
			"email" : "pledger@colorado.edu",
			"role" : "Developer"
		},
        {
            "name" : "Evert Pot",
            "email" : "evert@rooftopsolutions.nl",
            "homepage" : "http://www.rooftopsolutions.nl/",
            "role" : "Original Developer"
        }
    ],
    "autoload" : {
        "psr-0" : {
            "PHPDocMD" : "src/"
        }
    },
    "bin" : [
        "bin/phpdocmd"
    ],
    "config" : {
        "bin-dir" : "bin"
    }
}
